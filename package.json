{
  "name": "nodejs_express_template",
  "version": "1.0.0",
  "description": "Express Server Template for NodeJS",
  "main": "./src/server.js",
  "scripts": {
    "test": "mocha test/**/*.js",
    "start": "node ./src/server.js",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js ./src/server.js",
    "precommit": "lint-staged",
    "lint": "eslint \"**/*.js\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephendsmithco/NodeJS_Express_Template.git"
  },
  "author": "Stephen Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stephendsmithco/NodeJS_Express_Template/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "express": "^4.17.3",
    "swagger-ui-express": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "standard": "^14.1.0"
  },
  "homepage": "https://github.com/stephendsmithco/NodeJS_Express_Template#README"
}
